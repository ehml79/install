CONF_DIR=/data/service/grafana/conf

CONF_FILE=/data/service/grafana/conf/grafana.ini

RESTART_ON_UPGRADE=true

PLUGINS_DIR=/data/service/grafana/data/plugins

PROVISIONING_CFG_DIR=/data/service/grafana/conf/provisioning

# Only used on systemd systems
PID_FILE_DIR=/data/service/grafana/run
